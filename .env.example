PHP_EXT_GD=false
PHP_EXT_GETTEXT=false
PHP_EXT_GMP=false
PHP_EXT_IMAP=false
PHP_EXT_INTL=false
PHP_EXT_LDAP=false
PHP_EXT_MYSQLI=false
PHP_EXT_OPCACHE=false
PHP_EXT_PCNTL=false
PHP_EXT_PDO_DBLIB=false
PHP_EXT_PDO_MYSQL=false
PHP_EXT_PDO_ODBC=false
PHP_EXT_PDO_PGSQL=false
PHP_EXT_PGSQL=false
PHP_EXT_SHMOP=false
PHP_EXT_SNMP=false
PHP_EXT_SOAP=false
PHP_EXT_SOCKETS=false
PHP_EXT_SYSCMSG=false
PHP_EXT_SYSVSEM=false
PHP_EXT_SYSVSHM=false
PHP_EXT_TIDY=false
