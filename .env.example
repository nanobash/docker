##############################################################################
#                 .env config file for docker-compose.yaml                   #
#                                                                            #
#                                  USAGE                                     #
#                           cp .env.example .env                             #
##############################################################################

# Basic application configs
WEB_ROOT=../webroot

# --- Redis --- #
DOCKER_REDIS_PORT=6379

# --- PostgreSQL --- #
DOCKER_POSTGRES_USER=postgres
DOCKER_POSTGRES_PASSWORD=postgres
DOCKER_POSTGRES_PORT=5432

# NGINX service configuration
DOCKER_NGINX_HTTP_PORT=80
DOCKER_NGINX_HTTPS_PORT=443
DOCKER_NGINX_DEFAULT=default.conf
DOCKER_NGINX_LARAVEL=laravel.conf
DOCKER_NGINX_SYMFONY=symfony.conf

# PHP service configuration
PHP_EXT_APCU=false
PHP_EXT_BCMATH=false
PHP_EXT_BZ2=false
PHP_EXT_CALENDAR=false
PHP_EXT_DBA=false
PHP_EXT_DOM=false
